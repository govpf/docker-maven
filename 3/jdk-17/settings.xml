<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <profiles>
    <profile>
      <repositories>
  <repository>
    <id>central-apache</id>
    <name>Central Repository</name>
    <layout>default</layout>
    <url>https://repo1.maven.org/maven2/</url>
    <snapshots>
      <enabled>false</enabled>
    </snapshots>
  </repository>
  <repository>
    <id>central</id>
    <name>Maven Repository Switchboard</name>
    <layout>default</layout>
    <url>https://repo1.maven.org/maven2</url>
    <snapshots>
      <enabled>false</enabled>
    </snapshots>
  </repository>
       <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release-public</id>
          <name>libs-release</name>
          <url>https://bin.gov.pf/artifactory/libs-release-local-public</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>snapshots</id>
          <name>snapshot-public</name>
          <url>https://bin.gov.pf/artifactory/libs-snapshot-local-public</url>
        </repository>
      </repositories>
      <id>artifactory</id>
    </profile>
    <profile>
      <id>intranet</id>
      <activation>
        <property>
          <name>env.MAVEN_PROFILE</name>
          <value>intranet</value>
        </property>
      </activation>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release-public</id>
          <name>libs-release</name>
          <url>https://bin.gov.pf/artifactory/libs-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>snapshots</id>
          <name>snapshot-public</name>
          <url>https://bin.gov.pf/artifactory/libs-snapshot</url>
        </repository>
        <repository>
          <id>central-apache</id>
          <name>Central Repository</name>
          <layout>default</layout>
          <url>https://repo1.maven.org/maven2/</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>central</id>
          <name>Maven Repository Switchboard</name>
          <layout>default</layout>
          <url>https://repo1.maven.org/maven2</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
      </repositories>
    </profile>
    <profile>
      <id>aws</id>
      <activation>
        <property>
          <name>env.MAVEN_PROFILE</name>
          <value>aws</value>
        </property>
      </activation>
      <repositories>
        <repository>
          <id>central-apache</id>
          <name>Central Repository</name>
          <layout>default</layout>
          <url>https://repo1.maven.org/maven2/</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>central</id>
          <name>Maven Repository Switchboard</name>
          <layout>default</layout>
          <url>https://repo1.maven.org/maven2</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release-public</id>
          <name>libs-release</name>
          <url>https://bin.gov.pf/artifactory/libs-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>snapshots</id>
          <name>snapshot-public</name>
          <url>https://bin.gov.pf/artifactory/libs-snapshot</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
